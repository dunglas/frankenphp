{
	# Debug
    {$CADDY_DEBUG}

	frankenphp {
		#worker /path/to/your/worker.php
		{$FRANKENPHP_CONFIG}
	}
}

{$SERVER_NAME:localhost}

log
route {
	 
	@frankenphp path *.php {
		root public/
		index {path}/index.php 
		resolve_root_symlink
	}
	
	php @phpFiles
	encode zstd gzip
	file_server

	respond 404
}
